{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev --prefix ../web\" \"electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.2.4"
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "electron-builder": "^22.10.5"
  },
  "build": {
    "appId": "br.com.reflow.app",
    "mac": {
      "category": "your.app.category.type"
    }
  }
}
